# Database URL (Prisma Postgres from Vercel Marketplace)
# This will be auto-configured when you add Prisma from Vercel Marketplace
DATABASE_URL="postgresql://user:password@host:5432/database"

# ElevenLabs API Key (get from https://elevenlabs.io)
ELEVENLABS_API_KEY="your-elevenlabs-api-key"

# Cartesia API Key (get from https://cartesia.ai)
CARTESIA_API_KEY="your-cartesia-api-key"

# Admin password for /admin routes (leave empty to disable auth in development)
# In production, always set this!
ADMIN_PASSWORD="your-secure-admin-password"
